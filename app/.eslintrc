{
  "rules": {
    "no-var": 2,
    "react/sort-comp": [1, {
      "order": [
        "mixins",
        "lifecycle",
        "everything-else",
        "rendering"
      ],
      "groups": {
        mixins: [
          "mixins",
          "stores",
          "/^getStateFor.+$/"
        ],
        rendering: [
          "render",
          "/^render.+$/"
        ]
      }
    }]
  }
}